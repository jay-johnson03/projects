<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.effect.Bloom?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.identification.LogInController" prefHeight="400.0" prefWidth="600.0">
    <children>
        <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0">
            <children>
                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="51.0" prefWidth="255.0" text="Log In">
                    <font>
                        <Font name="System Bold Italic" size="30.0" />
                    </font>
                    <effect>
                        <Bloom />
                    </effect>
                </Label>
                <Label alignment="CENTER" prefHeight="31.0" prefWidth="83.0" text="Email :" />
                <TextField fx:id="textEmail" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="26.0" prefWidth="426.0" />
                <Label alignment="CENTER" prefHeight="35.0" prefWidth="109.0" text="Password : " />
                <TextField fx:id="textFieldPassword" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="26.0" prefWidth="429.0" />
                <Button fx:id="loginConfirm" onAction="#setLogAction" prefHeight="33.0" prefWidth="65.0" text="Log In" />
            </children>
        </VBox>
        <VBox />
    </children>
</AnchorPane>
