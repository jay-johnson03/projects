<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.Bloom?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.identification.LogInController">
    <children>
        <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0">
            <children>
                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="51.0" prefWidth="255.0" text="Welcome Back User! ">
                    <font>
                        <Font name="System Bold Italic" size="30.0" />
                    </font>
                    <effect>
                        <Bloom />
                    </effect>
                </Label>
                <Label fx:id="userName" alignment="CENTER" contentDisplay="BOTTOM" prefHeight="31.0" prefWidth="83.0" text="User: ">
               <font>
                  <Font name="System Italic" size="20.0" />
               </font></Label>
                <Label fx:id="userEmail" alignment="CENTER" prefHeight="35.0" prefWidth="109.0" text="Email : ">
               <font>
                  <Font size="15.0" />
               </font></Label>
            </children>
        </VBox>
        <VBox />
    </children>
</AnchorPane>
